@model TradeTicketDto

<ul class="nav nav-tabs pt-2" id="tabs" role="tablist">
    <li class="nav-item">
        <a class="nav-link active" id="trade-ticket-tab" data-toggle="tab" href="#trade-ticket" role="tab" aria-controls="trade-ticket" aria-selected="true">Trade Ticket</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="trades-tab" data-toggle="tab" href="#trades" role="tab" aria-controls="trades" aria-selected="false">Trades</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="trade-day-performance-tab" data-toggle="tab"
           href="#trade-day-performance" role="tab" aria-controls="trade-day-performance" aria-selected="false">Day Perf</a>
    </li>
</ul>
<div class="tab-content pt-2" id="strategiesTabContent">
    <div class="tab-pane fade show active" id="trade-ticket" role="tabpanel" aria-labelledby="trade-ticket-tab">
        @Html.Partial("_TradeTicketForm", Model)
    </div>
    <div class="tab-pane fade" id="trades" role="tabpanel" aria-labelledby="trades-tab">
        @Html.Partial("_TradeTicketsTable")
    </div>
    <div class="tab-pane fade" id="trade-day-performance" role="tabpanel" aria-labelledby="trade-day-performance-tab">
        @Html.Partial("_TradeDayPerformanceTable")
    </div>
</div>
