@model PullbackTradeTicketDto

<ul class="nav nav-tabs pt-2" id="pullbackTabs" role="tablist">
    <li class="nav-item">
        <a class="nav-link active" id="trade-ticket-tab" data-toggle="tab" href="#trade-ticket" role="tab" aria-controls="trade-ticket" aria-selected="true">Trade Ticket</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="pullback-trades-tab" data-toggle="tab" href="#pullback-trades" role="tab" aria-controls="pullback-trades" aria-selected="false">Trades</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="pullback-trade-day-performance-tab" data-toggle="tab"
           href="#pullback-trade-day-performance" role="tab" aria-controls="pullback-trade-day-performance" aria-selected="false">Day Perf</a>
    </li>
</ul>
<div class="tab-content pt-2" id="strategiesTabContent">
    <div class="tab-pane fade show active" id="trade-ticket" role="tabpanel" aria-labelledby="trade-ticket-tab">
        @Html.Partial("_PullbackTradeTicketForm", Model)
    </div>
    <div class="tab-pane fade" id="pullback-trades" role="tabpanel" aria-labelledby="pullback-trades-tab">
        @Html.Partial("_PullbackTradeTicketsTable")
    </div>
    <div class="tab-pane fade" id="pullback-trade-day-performance" role="tabpanel" aria-labelledby="pullback-trade-day-performance-tab">
        @Html.Partial("_PullbackTradeDayPerformanceTable")
    </div>
</div>
